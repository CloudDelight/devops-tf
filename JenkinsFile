pipeline {
    agent any
     tools {
      terraform "Terraform1.0.0"
      }
    stages {
        stage ('terraform plan') {
       steps {
         withAWS(credentials: 'test-user', region: 'us-east-1')
           sh "terraform -v"
           sh "terraform init"
           sh "terraform plan"
       } 
    }
    }
}

